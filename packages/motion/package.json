{
  "name": "motion-v",
  "version": "0.5.2-beta.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/unovue/motion-vue",
  "repository": {
    "type": "git",
    "url": "https://github.com/unovue/motion-vue.git"
  },
  "keywords": [
    "vue",
    "motion",
    "motionone"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/es/index.mjs",
      "require": "./dist/cjs/index.js"
    },
    "./nuxt": {
      "import": {
        "types": "./dist/nuxt/index.d.mts",
        "default": "./dist/nuxt/index.mjs"
      },
      "require": {
        "types": "./dist/nuxt/index.d.cts",
        "default": "./dist/nuxt/index.cjs"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.mjs",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*/index.d.ts",
        "./dist/src/index.d.ts"
      ],
      "nuxt": [
        "./dist/nuxt/index.d.mts",
        "./dist/nuxt/index.d.ts"
      ]
    }
  },
  "files": [
    "./LICENSE",
    "./dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "rm -rf dist && vite build",
    "test": "vitest --dom",
    "coverage": "vitest run --coverage",
    "pub:release": "pnpm publish --access public"
  },
  "dependencies": {
    "@vueuse/core": "^12.0.0",
    "framer-motion": "11.15.0",
    "hey-listen": "^1.0.8",
    "motion-dom": "^11.14.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "framer-motion": "^11.15.0",
    "happy-dom": "^16.0.1",
    "jsdom": "^24.0.0",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.4",
    "vitest": "^1.4.0"
  }
}
